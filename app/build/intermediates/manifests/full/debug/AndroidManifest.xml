<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ambulance.user"
    android:versionCode="21"
    android:versionName="4" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="27" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NOTIFICATION_POLICY" />
    <uses-permission android:name="android.permission.BIND_NOTIFICATION_LISTENER_SERVICE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.scriptsmall.cabbookphp.permission.C2D_MESSAGE" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <!-- Permissions required for GCM -->
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" /> <!-- Include required permissions for Google Maps API to run. -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <permission
        android:name="com.ambulance.user.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.ambulance.user.permission.C2D_MESSAGE" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/new_app_icon"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.scriptmall.cabuser.SplashActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.scriptmall.cabuser.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.scriptmall.cabuser.RegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.Register2Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.Register3Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.Register4Activity"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyDrJlYmi86il9sTTZOrXTk8uGD5jkQr2LY" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <service
            android:name="com.scriptmall.cabuser.FirebaseMessagingService"
            android:enabled="true"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service
            android:name="com.scriptmall.cabuser.FirebaseInstanceIDService"
            android:enabled="true"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service
            android:name="com.scriptmall.cabuser.ScriptsService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name="com.scriptmall.cabuser.ConfrimActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.LaterConfrimActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.OutstationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.RentalActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.MyRidesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.MyRidesCancelActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.ResultActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.TrackingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.ProfileActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.EditprofileActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.PasswordUpdateActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.SupportActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.SupportAnsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.RateCardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.TrackOthersActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.Tracking2Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.TrackerRideListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.scriptmall.cabuser.BillActivity"
            android:screenOrientation="portrait" />
        <!--
         FirebaseMessagingService performs security checks at runtime,
             no need for explicit permissions despite exported="true"
        -->
        <service
            android:name="com.google.firebase.messaging.FirebaseMessagingService"
            android:exported="true" >
            <intent-filter android:priority="-500" >
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name="com.google.android.gms.common.api.GoogleApiActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" /> <!-- Include the AdActivity and InAppPurchaseActivity configChanges and themes. -->
        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />

        <receiver
            android:name="com.google.android.gms.measurement.AppMeasurementReceiver"
            android:enabled="true"
            android:exported="false" >
        </receiver>
        <receiver
            android:name="com.google.android.gms.measurement.AppMeasurementInstallReferrerReceiver"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.INSTALL_PACKAGES" >
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.google.android.gms.measurement.AppMeasurementService"
            android:enabled="true"
            android:exported="false" />
        <service
            android:name="com.google.android.gms.measurement.AppMeasurementJobService"
            android:enabled="true"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <receiver
            android:name="com.google.firebase.iid.FirebaseInstanceIdReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />

                <category android:name="com.ambulance.user" />
            </intent-filter>
        </receiver>
        <!--
 Internal (not exported) receiver used by the app to start its own exported services
             without risk of being spoofed.
        -->
        <receiver
            android:name="com.google.firebase.iid.FirebaseInstanceIdInternalReceiver"
            android:exported="false" />
        <!--
 FirebaseInstanceIdService performs security checks at runtime,
             no need for explicit permissions despite exported="true"
        -->
        <service
            android:name="com.google.firebase.iid.FirebaseInstanceIdService"
            android:exported="true" >
            <intent-filter android:priority="-500" >
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <provider
            android:name="com.google.firebase.provider.FirebaseInitProvider"
            android:authorities="com.ambulance.user.firebaseinitprovider"
            android:exported="false"
            android:initOrder="100" />
    </application>

</manifest>